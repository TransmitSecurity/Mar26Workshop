{
  "name": "@transmitsecurity-dev/ts-demo-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite -m dev",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "build-test": "npm run build && npm run test:headless",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:e2e",
    "test:headless": "npm run test:unit && npm run test:e2e:headless",
    "test:unit": "vitest run --dom",
    "test:unit:watch": "vitest --dom",
    "test:e2e": "playwright test",
    "test:e2e:headless": "npm run test:e2e:desktop && npm run test:e2e:mobile",
    "test:e2e:desktop": "playwright test --config=playwright.desktop.headless.config.ts",
    "test:e2e:mobile": "playwright test --config=playwright.mobile.headless.config.ts",
    "e2e:report": "npx playwright show-report e2e-test-results",
    "e2e:report:mobile": "npx playwright show-report e2e-mobile-test-results",
    "e2e:report:desktop": "npx playwright show-report e2e-desktop-test-results",
    "vite:inspect": "npx serve .vite-inspect",
    "rollup:inspect": "npx serve .rollup-inspect",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "help": "node src/scripts/log-help.js",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\""
  },
  "dependencies": {
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-vue": "^2.0.0",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@transmitsecurity-dev/ts-demo-client-lib": "file:client-lib",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^6.0.2",
    "axios": "^1.5.1",
    "daisyui": "^2.51.5",
    "flag-icons": "^6.11.1",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vite-plugin-pwa": "^0.14.7",
    "vue": "^3.3.4",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-otp-input": "^0.4.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.17.11",
    "@types/vuelidate": "^0.7.18",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cspell": "^6.31.2",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^9.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-inspect": "^0.7.24",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0",
    "ws": "^8.13.0"
  }
}
